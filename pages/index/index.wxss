/**index.wxss**/

/* 轮播图 */
swiper image{
  width: 100%;
  height: 100%;
}

/* 导航栏 */
.menus{
  display: flex;
  justify-content: space-around;
  padding: 20rpx 0;
}

.menus image{
  width: 128rpx;
  height: 140rpx;
}

/* 楼层 */
.floor-title image{
  width: 100%;
  height: 60rpx;
}
.floor{
  display: flex;
  justify-content: space-between;
  padding: 20rpx;
}
.floor .left image{
  width: 232rpx;
  height: 390rpx;
}
.floor .right{
  flex:1;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-left: 20rpx;
}
.floor .right image{
  width:225rpx;
  height: 190rpx;
}